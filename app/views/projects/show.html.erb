<%= link_to "Create New Project", new_project_path %>
<h1>All Projects</h1>

<h1><%= @project.name%></h1>
<p><%=@project.description%></p>
<ul>
  <li><%= link_to "Edit", edit_project_path(@project) %></li>
  <li><%= link_to "Destroy", project_path(@project), data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"
                  } %></li>
</ul>
